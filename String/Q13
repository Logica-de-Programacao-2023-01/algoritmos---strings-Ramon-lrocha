package main

import (
	"fmt"
	"strconv"
	"strings"
)

func main() {
	var texto string
	fmt.Print("Digite uma string: ")
	fmt.Scanln(&texto)

	sequencia := strings.Split(texto, "")
	sequenciaValida := true

	for i := 0; i < len(sequencia)-1; i++ {
		num1, _ := strconv.Atoi(sequencia[i])
		num2, _ := strconv.Atoi(sequencia[i+1])

		if num1 >= num2 {
			sequenciaValida = false
			break
		}
	}

	if sequenciaValida {
		fmt.Println("A string é uma sequência numérica crescente.")
	} else {
		fmt.Println("A string não é uma sequência numérica crescente.")
	}
}
