package main

import (
	"fmt"
	"strings"
)

func main() {
	var texto string
	fmt.Print("Digite uma string: ")
	fmt.Scanln(&texto)

	letrasUnicas := ""
	for _, char := range texto {
		if strings.Count(texto, string(char)) == 1 {
			letrasUnicas += string(char)
		}
	}

	fmt.Println("Letras Ãºnicas:", letrasUnicas)
}
